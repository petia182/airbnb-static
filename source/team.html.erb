<div class="container text-center">
  <h1>Our Team</h1>
  <ul class="list-inline text-center">
    <% ["will", "petya"].each do |member| %>
      <li>
      <h3><%= member.capitalize %></h3>
        <%= image_tag "#{member}.jpg", class: "avatar-large" %>
      </li>
    <% end %>
  </ul>
  <h3 class="padded">
    <i class="fa fa-envelope"></i>
    <%= link_to "contact@airbnb.com", "mailto:contact@airbnb.com" %>
  </h3>
</div>
